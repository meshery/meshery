{
  "kind": "edge",
  "metadata": {
    "description": "A relationship that allows traffic from one Pod to another Pod via a NetworkPolicy.",
    "styles": {
      "primaryColor": "",
      "svgColor": "",
      "svgWhite": ""
    },
    "isAnnotation": false
  },
  "schemaVersion": "relationships.meshery.io/v1alpha3",
  "selectors": [
    {
      "allow": {
        "from": [
          {
            "id": null,
            "kind": "Pod",
            "match": {
              "from": [
                {
                  "id": null,
                  "kind": "self",
                  "mutatedRef": [
                    [
                      "configuration",
                      "spec",
                      "_",
                      "networkPolicy"
                    ]
                  ]
                }
              ],
              "to": [
                {
                  "id": null,
                  "kind": "NetworkPolicy",
                  "mutatorRef": [
                    [
                      "displayName"
                    ]
                  ]
                }
              ]
            },
            "patch": null
          }
        ],
        "to": [
          {
            "id": null,
            "kind": "Pod",
            "match": {
              "from": [
                {
                  "id": null,
                  "kind": "NetworkPolicy",
                  "mutatedRef": [
                    [
                      "configuration",
                      "spec",
                      "ingress"
                    ],
                    [
                      "configuration",
                      "spec",
                      "selector",
                      "matchLabels"
                    ]
                  ]
                }
              ],
              "to": [
                {
                  "id": null,
                  "kind": "self",
                  "mutatorRef": [
                    [
                      "displayName"
                    ],
                    [
                      "configuration",
                      "metadata",
                      "labels"
                    ]
                  ]
                }
              ]
            },
            "patch": null
          }
        ]
      }
    }
  ],
  "subType": "firewall",
  "status": "enabled",
  "type": "binding",
  "version": "v1.0.0"
}