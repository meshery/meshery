{
  "name": "Meshery",
  "provider": "Layer5, Inc.",
  "icon": "meshery-logo-white.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "extension-meshery.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "Meshery",
      "src": "/index.html",
      "root": "/ui",
      "backend": {
        "socket": "extension-meshery.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/mesheryctl/darwin/mesheryctl"
          }
        ],
        "windows": [
          {
            "path": "/mesheryctl/windows/mesheryctl.exe"
          }
        ],
        "linux": [
          {
            "path": "/mesheryctl/linux/mesheryctl"
          }
        ]
      }
    ]
  }
}
