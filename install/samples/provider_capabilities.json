      "endpoint": "/api/identity/users/anonymous"
    }
  ],
  "redirects": {
    "/": "/extension/meshmap",
    "": "/extension/meshmap"
  },
  "restrictedAccess": {
    "isMesheryUiRestricted": true,
    "allowedComponents": {
      "navigator": {
        "help": true,
        "lifecycle": {},
        "configuration": {}
      },
      "header": {
        "contextSwitcher": true,
        "notifications": true,
        "profile": true
      }
    }
  }
}
