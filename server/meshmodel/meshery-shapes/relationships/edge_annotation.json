          },
          {
            "kind": "*",
            "model": "gcp"
          },
          {
            "kind": "*",
            "model": "meshery-flowchart"
          }
        ],
        "to": [
          {
            "kind": "*",
            "model": "meshery-shapes"
          },
          {
            "kind": "*",
            "model": "aws"
          },
          {
            "kind": "*",
            "model": "gcp"
          },
          {
            "kind": "*",
            "model": "meshery-flowchart"
          }
        ]
