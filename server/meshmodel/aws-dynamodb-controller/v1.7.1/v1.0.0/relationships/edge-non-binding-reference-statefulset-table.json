{
  "id": "00000000-0000-0000-0000-000000000000",
  "schemaVersion": "relationships.meshery.io/v1alpha3",
  "version": "v1.0.0",
  "kind": "edge",
  "type": "non-binding",
  "subType": "reference",
  "evaluationQuery": "edge_non_binding_reference_relationship",
  "status": "enabled",
  "metadata": {
    "description": "Reference relationship from AWS DynamoDB Table to Kubernetes StatefulSet. This represents applications referencing DynamoDB connection strings or credentials via StatefulSet configuration while targeting a given database table.",
    "capabilities": {
      "designer": {
        "edit": {
          "style": true,
          "config": false,
          "label": true
        }
      }
    }
  },
  "model": {
    "schemaVersion": "models.meshery.io/v1beta1",
    "version": "v1.7.1",
    "name": "aws-dynamodb-controller",
    "registrant": {
      "kind": "artifacthub"
    },
    "category": {
      "name": "Database"
    },
    "displayName": "AWS DynamoDB Controller"
  },
  "selectors": [
    {
      "allow": {
        "from": [
          {
            "kind": "Table",
            "model": {
              "name": "aws-dynamodb-controller",
              "registrant": {
                "kind": "artifacthub"
              }
            },
            "patch": {
              "patchStrategy": "replace",
              "mutatorRef": [
                ["displayName"]
              ],
              "description": "DynamoDB Table endpoint can be referenced by Kubernetes StatefulSet configuration."
            }
          }
        ],
        "to": [
          {
            "kind": "StatefulSet",
            "model": {
              "name": "kubernetes",
              "registrant": {
                "kind": "artifacthub"
              }
            },
            "patch": {
              "patchStrategy": "replace",
              "mutatedRef": [
                ["spec", "template", "spec", "containers", "_", "env"]
              ],
              "description": "StatefulSet containers can store DynamoDB connection strings or credentials used for accessing the database table."
            }
          }
        ]
      },
      "deny": {
        "from": [],
        "to": []
      }
    }
  ]
}
