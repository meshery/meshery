    "version": "aws.zakariaamine.com/v1alpha1",
    "kind": "AWSECRCredential",
    "schema": "{\n \"description\": \"AWSECRCredential is the Schema for the awsecrcredentials API.\\nIt manages several docker secrets for AWS ECR across different namespaces.\",\n \"properties\": {\n  \"spec\": {\n   \"description\": \"AWSECRCredentialSpec defines the desired state of AWSECRCredential\",\n   \"properties\": {\n    \"awsAccess\": {\n     \"description\": \"the name of the secret holding the AWS credentials that will be used to talk to AWS to get ECR credentials\",\n     \"properties\": {\n      \"accessKeyId\": {\n       \"description\": \"AWS access key associated with an IAM account that will be used to create and refresh ECR docker credentials\",\n       \"type\": \"string\"\n      },\n      \"region\": {\n       \"description\": \"specifies the AWS Region to send the request to\",\n       \"type\": \"string\"\n      },\n      \"secretAccessKey\": {\n       \"description\": \"the secret key associated with the access key.\",\n       \"type\": \"string\"\n      }\n     },\n     \"required\": [\n      \"accessKeyId\",\n      \"region\",\n      \"secretAccessKey\"\n     ],\n     \"type\": \"object\"\n    },\n    \"namespaces\": {\n     \"description\": \"the namespaces in which the operator will create and and manage ECR registry docker secrets\",\n     \"items\": {\n      \"type\": \"string\"\n     },\n     \"minItems\": 1,\n     \"type\": \"array\"\n    },\n    \"secretName\": {\n     \"description\": \"the name of the docker secret that will be created and updated by the operator in each of the specified namespaces\",\n     \"maxLength\": 253,\n     \"minLength\": 1,\n     \"type\": \"string\"\n    }\n   },\n   \"type\": \"object\"\n  }\n },\n \"title\": \"AWSECR Credential\",\n \"type\": \"object\"\n}"
  }
