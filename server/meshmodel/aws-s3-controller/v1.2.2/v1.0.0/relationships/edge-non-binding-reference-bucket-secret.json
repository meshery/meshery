{
  "id": "00000000-0000-0000-0000-000000000000",
  "schemaVersion": "relationships.meshery.io/v1alpha3",
  "version": "v1.0.0",
  "kind": "edge",
  "type": "non-binding",
  "subType": "reference",
  "evaluationQuery": "edge_non_binding_reference_relationship",
  "status": "enabled",
  "metadata": {
    "description": "Reference relationship from AWS S3 Bucket to Kubernetes Secret. This represents applications referencing bucket access credentials or signed URL material via Secrets while targeting a given S3 Bucket.",
    "capabilities": {
      "designer": {
        "edit": {
          "style": true,
          "config": false,
          "label": true
        }
      }
    }
  },
  "model": {
    "schemaVersion": "models.meshery.io/v1beta1",
    "version": "v1.0.0",
    "name": "aws-s3-controller",
    "registrant": {
      "kind": "artifacthub"
    },
    "category": {
      "name": "Provisioning"
    },
    "displayName": "AWS S3 Controller"
  },
  "selectors": [
    {
      "allow": {
        "from": [
          {
            "kind": "Bucket",
            "model": {
              "name": "aws-s3-controller",
              "registrant": {
                "kind": "artifacthub"
              }
            },
            "patch": {
              "patchStrategy": "replace",
              "mutatorRef": [
                ["displayName"]
              ],
              "description": "S3 Bucket identity can be referenced by Kubernetes secrets-based configuration."
            }
          }
        ],
        "to": [
          {
            "kind": "Secret",
            "model": {
              "name": "kubernetes",
              "registrant": {
                "kind": "artifacthub"
              }
            },
            "patch": {
              "patchStrategy": "replace",
              "mutatedRef": [
                ["data"]
              ],
              "description": "Secret data can store S3 credentials or tokens used for accessing objects in the bucket."
            }
          }
        ]
      },
      "deny": {
        "from": [],
        "to": []
      }
    }
  ]
}
