{
    "apiVersion": "core.meshery.io/v1alpha1",
    "kind": "Peer",
    "metadata": {
      "name": "Configurational Peer Relationship",
      "description": "A relationship that defines ConfigMap as a configuration source for other Kubernetes objects.",
      "labels": {
        "type": "Configurational",
        "category": "Kubernetes",
        "version": "v1.25.2"
      }
    },
    "model": {
      "name": "kubernetes",
      "version": "v1.25.2",
      "displayName": "Kubernetes",
      "category": {
        "name": "Orchestration & Management",
        "metadata": null
      },
      "metadata": {}
    },
    "subType": "Configurational",
    "selectors": {
      "allow": {
        "from": [
          {
            "kind": "ConfigMap",
            "model": "kubernetes"
          }
        ],
        "to": [
          {
            "kind": "Pod",
            "model": "kubernetes"
          },
          {
            "kind": "Deployment",
            "model": "kubernetes"
          },
          {
            "kind": "Service",
            "model": "kubernetes"
          },
          {
            "kind": "RuntimeClass",
            "model": "kubernetes"
          },
          {
            "kind": "Endpoints",
            "model": "kubernetes"
          },
          {
            "kind": "Ingress",
            "model": "kubernetes"
          }
        ]
      },
      "deny": {
        "from": [],
        "to": []
      }
    }
  }
  