  "component": {
   "kind": "Customer",
   "schema": "{\n \"description\": \"Customer is the Schema for the customers API\",\n \"properties\": {\n  \"spec\": {\n   \"description\": \"CustomerSpec defines the desired state of Customer\",\n   \"properties\": {\n    \"email\": {\n     \"description\": \"Email of the customer\",\n     \"pattern\": \"\\\\S+@\\\\S+\\\\.\\\\S+\",\n     \"type\": \"string\"\n    },\n    \"legacyCustomer\": {\n     \"description\": \"LegacyCustomer is a boolean flag that represents whether a customer is created using legacy API or not In case of a legacy Customer, a signup link is sent to the customer email which he can than use to signup In case of a normal Customer, no signup link is sent to the customer. The customer than has to signup manually using the portal If not given, default behaviour is false i.e. normal customer\",\n     \"type\": \"boolean\"\n    },\n    \"name\": {\n     \"description\": \"Name of the customer\",\n     \"type\": \"string\"\n    },\n    \"projects\": {\n     \"description\": \"List of ProjectKeys in which customer will be added\",\n     \"items\": {\n      \"type\": \"string\"\n     },\n     \"minItems\": 1,\n     \"type\": \"array\"\n    }\n   },\n   \"required\": [\n    \"email\",\n    \"name\",\n    \"projects\"\n   ],\n   \"type\": \"object\"\n  }\n },\n \"title\": \"Customer\",\n \"type\": \"object\"\n}",
   "version": "jiraservicedesk.stakater.com/v1alpha1"
  },
